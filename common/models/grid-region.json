{
  "name": "GridRegion",
  "base": "Model",
  "idInjection": false,
  "abstract": true,
  "options": { },
  "properties": {
    "id": {
      "id": true,
      "type": "string",
      "defaultFn": "guid",
      "required": false
    },
    "kind": {
      "type": "string",
      "required": false,
      "pattern": "/^(open|word|short|blocked)$/"
    },
    "orientation": {
      "type": "string",
      "required": true,
      "pattern": "/^horizontal$|^vertical$/"
    }
  },
  "validations": [],
  "relations": {
    "head": {
      "type": "belongsTo",
      "model": "CellOrientation",
      "foreignKey": "headId"
    },
    "tail": {
      "type": "belongsTo",
      "model": "CellOrientation",
      "foreignKey": "tailId"
    },
    "cells": {
      "type": "hasMany",
      "model": "CellOrientation",
      "foreignKey": "regionId"
    }
  },
  "acls": [],
  "methods": {}
}
