{
  "initial:before": {
    "loopback#favicon": {}
  },
  "initial": {
    "compression": {
      "enabled": false
    },
    "cors": {
      "enabled": true,
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "session": {},
  "auth": {},
  "parse": {},
  "routes": {
    "loopback#status": {
      "enabled": true,
      "paths": [
        "/status"
      ]
    },
    "loopback#rest": {
      "enabled": true,
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "routes:after": {
    "connect-livereload": { }
  },
  "files": {
    "loopback#static": {
      "enabled": true,
      "params": "$!../client"
    }
  },
  "files:after": {
    "./middleware/serve-index": {
      "enabled": true,
      "params": {
        "indexFile": "${indexFile}"
      },
      "paths": [
        "/\/[^./]*$/",
        "/"
      ]
    }
  },
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "strong-error-handler": {}
  }
}
